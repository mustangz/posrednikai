      const formWrapper = document.getElementById('formWrapper');
      const submitBtn = document.getElementById('submitBtn');

      form.addEventListener('submit', async function(e) {
        e.preventDefault();

        submitBtn.disabled = true;
        submitBtn.textContent = 'Wysy≈Çam...';
        formWrapper.classList.remove('error');

        try {
          const formData = new FormData(form);
          const response = await fetch('https://api.web3forms.com/submit', {
            method: 'POST',
            body: formData
          });

          const data = await response.json();

          if (data.success) {
            if (window.va) {
              window.va('event', { name: 'presale_signup', data: { email: formData.get('email') } });
            }
            formWrapper.classList.add('success');
          } else {
            formWrapper.classList.add('error');
            submitBtn.disabled = false;
            submitBtn.textContent = 'Zapisujƒô siƒô na presale ‚Üí';
          }
        } catch (error) {
          formWrapper.classList.add('error');
          submitBtn.disabled = false;
          submitBtn.textContent = 'Zapisujƒô siƒô na presale ‚Üí';
        }
      });

      // FAQ accordion
      document.querySelectorAll('.faq-question').forEach(btn => {
        btn.addEventListener('click', () => {
          const item = btn.parentElement;
          const isActive = item.classList.contains('active');

          document.querySelectorAll('.faq-item').forEach(i => i.classList.remove('active'));

          if (!isActive) {
